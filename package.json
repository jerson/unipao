{
  "name": "unipao",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:android:beta": "cd android && ./gradlew assembleReleaseBeta",
    "build:windows:assets":
      "node node_modules/react-native/local-cli/cli.js bundle --platform windows --entry-file index.js --bundle-output windows\\unipao\\ReactAssets\\index.windows.bundle --assets-dest windows\\unipao\\ReactAssets  --dev false",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "windows": "node node_modules/react-native/local-cli/cli.js run-windows",
    "stats:android":
      "appcenter codepush deployment list -a jeral17/UniPAOAndroid",
    "stats:ios": "appcenter codepush deployment list -a jeral17/UniPAOIOS",
    "stats:windows":
      "appcenter codepush deployment list -a jeral17/UniPAOWindows",
    "push:ios:release":
      "appcenter codepush release-react --plist-file ios/upao/Info.plist -a jeral17/UniPAOIOS -d Release",
    "push:android:release":
      "appcenter codepush release-react -a jeral17/UniPAOAndroid -d Release",
    "push:windows:release":
      "appcenter codepush release-react -a jeral17/UniPAOWindows -d Release",
    "push:ios:beta":
      "appcenter codepush release-react --plist-file ios/upao/Info.plist -a jeral17/UniPAOIOS -d Beta",
    "push:android:beta":
      "appcenter codepush release-react -a jeral17/UniPAOAndroid -d Beta",
    "push:windows:beta":
      "appcenter codepush release-react -a jeral17/UniPAOWindows -d Beta",
    "push:all:release":
      "npm run push:android:release && npm run push:ios:release && npm run push:windows:release",
    "push:all:beta":
      "npm run push:android:beta && npm run push:ios:beta && npm run push:windows:beta",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "precommit": "lint-staged",
    "prettier": "prettier --write --single-quote 'src/**/*.tsx'"
  },
  "lint-staged": {
    "*.{tsx,ts,js,json,css}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@types/jquery": "^3.3.29",
    "@types/prop-types": "^15.7.0",
    "@types/pubsub-js": "^1.5.18",
    "@types/react": "^16.8.13",
    "@types/react-native": "^0.57.43",
    "@types/react-navigation": "^3.0.6",
    "babel-jest": "^24.7.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "1.17.0",
    "prop-types": "^15.7.2",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "16.8.3",
    "rnpm-plugin-windows": "^0.2.8",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "tslint-react-native": "^0.0.7",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.0",
    "@react-native-community/netinfo": "^2.0.0",
    "@react-native-community/slider": "^1.0.4",
    "@react-native-community/viewpager": "^1.1.5",
    "appcenter": "^1.13.0",
    "appcenter-analytics": "^1.13.0",
    "appcenter-crashes": "^1.13.0",
    "appcenter-push": "^1.13.0",
    "base-64": "^0.1.0",
    "cheerio": "^1.0.0-rc.2",
    "cheerio-without-node-native": "^0.20.2",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.7.0",
    "react": "16.8.3",
    "react-native": "0.59.4",
    "react-native-cancelable-fetch": "^0.1.1",
    "react-native-code-push": "^5.6.0",
    "react-native-custom-android-webview": "^1.0.5",
    "react-native-device-info": "^1.4.3",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-htmlview": "^0.13.0",
    "react-native-image-pan-zoom": "^2.1.11",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-modal": "^9.0.0",
    "react-native-modal-datetime-picker": "^6.1.0",
    "react-native-reanimated": "^1.0.0",
    "react-native-share": "^1.1.3",
    "react-native-vector-icons": "^6.4.2",
    "react-native-windows": "0.57.1",
    "react-navigation": "3.8.1",
    "react-navigation-tabs": "^2.1.1",
    "realm": "^2.26.1",
    "string-format": "^2.0.0",
    "underscore.string": "^3.3.5"
  }
}
