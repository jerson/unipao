{
    "name": "unipao",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "babel-preset-react-native-stage-0": "^1.0.1",
        "lint-staged": "^6.0.0",
        "prettier": "1.9.2",
        "react-test-renderer": "16.2.0",
        "rnpm-plugin-windows": "^0.2.8"
    },
    "scripts": {
        "start": "react-native start",
        "build:android": "cd android && ./gradlew assembleRelease",
        "build:windows:assets": "react-native bundle --platform windows --entry-file index.js --bundle-output windows\\unipao\\ReactAssets\\index.windows.bundle --assets-dest windows\\unipao\\ReactAssets  --dev false",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "windows": "react-native run-windows",
        "stats:android": "appcenter codepush deployment list -a jeral17/UniPAOAndroid",
        "stats:ios": "appcenter codepush deployment list -a jeral17/UniPAOIOS",
        "stats:windows": "appcenter codepush deployment list -a jeral17/UniPAOWindows",
        "push:ios": "appcenter codepush release-react --plist-file ios/upao/Info.plist -a jeral17/UniPAOIOS -d Release",
        "push:android": "appcenter codepush release-react -a jeral17/UniPAOAndroid -d Release",
        "push:windows": "appcenter codepush release-react -a jeral17/UniPAOWindows -d Release",
        "push:all": "npm run push:android && npm run push:ios && npm run push:windows",
        "test": "node node_modules/jest/bin/jest.js --watch",
        "precommit": "lint-staged",
        "prettier": "prettier --write --single-quote 'src/**/*.js'"
    },
    "lint-staged": {
        "*.{js,json,css}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "appcenter": "^1.1.0",
        "appcenter-analytics": "^1.1.0",
        "appcenter-crashes": "^1.1.0",
        "appcenter-push": "^1.1.0",
        "base-64": "^0.1.0",
        "cheerio": "^1.0.0-rc.2",
        "cheerio-without-node-native": "^0.20.2",
        "moment": "^2.20.1",
        "numeral": "^2.0.6",
        "prop-types": "^15.6.0",
        "pubsub-js": "^1.5.7",
        "react": "16.2.0",
        "react-native": "^0.51.0",
        "react-native-code-push": "^5.2.1",
        "react-native-device-info": "^0.13.0",
        "react-native-fence-html": "^1.0.6",
        "react-native-htmlview": "^0.12.1",
        "react-native-image-gallery": "^2.1.4",
        "react-native-image-zoom-viewer": "^2.0.18",
        "react-native-linear-gradient": "^2.4.0",
        "react-native-modal": "^4.1.1",
        "react-native-modal-datetime-picker": "^4.13.0",
        "react-native-share": "^1.0.23",
        "react-native-vector-icons": "^4.4.3",
        "react-native-windows": "0.50.0",
        "react-navigation": "^1.0.0-beta.22",
        "realm": "^2.1.1",
        "string-format": "^0.5.0",
        "underscore.string": "^3.3.4"
    }
}
